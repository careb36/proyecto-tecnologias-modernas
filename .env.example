# Variables de entorno para el proyecto
# Copia este archivo a .env y ajusta los valores según tu entorno

# Base de datos Oracle
DATABASE_URL=jdbc:oracle:thin:@oracle:1521:xe
DATABASE_USER=system
DATABASE_PASSWORD=oracle

# SonarQube
SONAR_HOST_URL=http://localhost:9000
SONAR_TOKEN=tu-token-sonarqube-aqui

# Grafana
GF_SECURITY_ADMIN_PASSWORD=admin

# Loki
LOKI_URL=http://loki:3100

# Docker Registry (para GitLab CI)
CI_REGISTRY=registry.gitlab.com
CI_REGISTRY_USER=tu-usuario-gitlab
CI_REGISTRY_PASSWORD=tu-password-gitlab

# Servidor de producción (para deploys)
PRODUCTION_SERVER=192.168.1.100
SSH_PRIVATE_KEY=clave-ssh-aqui

# Configuración de aplicación
SPRING_PROFILES_ACTIVE=docker
SERVER_PORT=8080

# Configuración de recursos
JAVA_OPTS=-Xmx512m -Xms256m
